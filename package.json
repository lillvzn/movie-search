{
  "name": "movies-app",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "axios": "^0.24.0",
    "axios-mock-adapter": "^1.20.0",
    "cypress": "^9.2.0",
    "history": "^5.2.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "styled-components": "^5.3.3",
    "use-debounce": "^7.0.1"
  },
  "scripts": {
    "test": "react-scripts test",
    "test:e2e": "cypress run",
